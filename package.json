{
  "name": "rinawarp",
  "version": "1.0.0",
  "description": "A modern, AI-powered terminal that enhances developer productivity",
  "main": "dist/src/index.js",
  "scripts": {
    "type-check:core": "tsc -p tsconfig.core.json --noEmit",
    "test:core": "jest --selectProjects core-api --runInBand",
    "ci:core": "npm run type-check:core && npm run test:core",
    "ci:nonblocking": "jest --selectProjects web,terminal,analytics || true",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "deploy:prod": "node scripts/deploy.ts",
    "deploy:railway": "node scripts/deploy-railway.ts",
    "seed": "ts-node scripts/seed-tiers.ts"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@prisma/client": "^6.14.0",
    "@sendgrid/mail": "^8.1.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pm2": "^6.0.8",
    "rate-limiter-flexible": "^4.0.0",
    "stripe": "^18.4.0",
    "ts-node": "^10.9.2",
    "winston": "^3.8.0"
  },
  "devDependencies": {
    "@sentry/node": "^10.5.0",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/cron": "^2.0.1",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^4.17.23",
    "@types/express-validator": "^2.20.33",
    "@types/handlebars": "^4.0.40",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.123",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.11",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yaml": "^1.9.6",
    "compression": "^1.8.1",
    "cron": "^4.3.3",
    "date-fns": "^4.1.0",
    "jest": "^29.5.0",
