{
  "name": "@rinawarp/desktop",
  "version": "0.1.0",
  "private": true,
  "description": "RinaWarp - AI-Powered Terminal",
  "author": "RinaWarp Technologies, LLC",
  "license": "UNLICENSED",
  "copyright": "Copyright (c) 2025 RinaWarp Technologies, LLC. All Rights Reserved.",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite",
    "start": "electron .",
    "build": "vite build",
    "build:prod": "tsc && vite build",
    "build:electron": "electron-builder",
    "rebuild": "electron-rebuild -f -w node-pty",
    "clean": "rimraf dist release"
  },
  "dependencies": {
    "@firebase/app": "^0.9.0",
    "@firebase/auth": "^1.3.0",
    "@firebase/firestore": "^4.3.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@progress/kendo-data-query": "^1.7.1",
    "@progress/kendo-drawing": "^1.0.0",
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-react-buttons": "^5.0.0",
    "@progress/kendo-react-charts": "^5.0.0",
    "@progress/kendo-react-dateinputs": "^5.0.0",
    "@progress/kendo-react-dialogs": "^11.4.0",
    "@progress/kendo-react-dropdowns": "^5.0.0",
    "@progress/kendo-react-form": "^11.4.0",
    "@progress/kendo-react-grid": "^11.4.0",
    "@progress/kendo-react-indicators": "^5.0.0",
    "@progress/kendo-react-inputs": "^5.0.0",
    "@progress/kendo-react-intl": "^5.0.0",
    "@progress/kendo-react-layout": "^5.0.0",
    "@progress/kendo-theme-default": "^5.0.0",
    "@rinawarp/terminal": "file:../terminal",
    "@stripe/stripe-js": "^2.1.0",
    "@tensorflow/tfjs": "^4.10.0",
    "@types/ws": "^8.5.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.5.0",
    "firebase-admin": "^11.10.0",
    "jsonwebtoken": "^9.0.0",
    "node-pty": "^1.0.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.3.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-ligatures": "^0.7.0",
    "xterm-addon-search": "^0.13.0",
    "xterm-addon-unicode11": "^0.6.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "electron": "26.0.0",
    "electron-builder": "^24.6.0",
    "icon-gen": "^5.0.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.34.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.0",
    "vite-plugin-electron": "^0.15.0"
  },
  "type": "module",
  "//build": {
    "appId": "com.rinawarptechnologies.rinawarp",
    "productName": "RinaWarp",
    "directories": {
      "output": "release",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "assets/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icons/generated/icon.icns",
      "darkModeSupport": true,
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "assets/icons/generated/icon.ico",
      "target": [
        "nsis",
        "portable"
      ],
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "icon": "assets/icons/generated",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
